package com.collection_ArrayList;
import java.util.Collections;
import java.util.LinkedList;
public class LinkedListPractical 
{
	public static void main(String[] args) 
	{
		LinkedList list1 =new LinkedList();
		list1.add(0,100);
		list1.add(200);
		list1.add(300);
		list1.add(1, 400);
		list1.add(500);
		System.out.println(list1);
		list1.addFirst(50);
		list1.addLast(600);
		System.out.println(list1);
		System.out.println(list1.contains(700));
		list1.removeFirst();
		System.out.println(list1);
		list1.removeLast();
		System.out.println(list1);
		Object o;
		//get first element in list
		System.out.println(list1.peek());
		System.out.println(list1.getFirst());
		System.out.println(list1.peekFirst());
		System.out.println(list1.element());
		//get last element in list
		System.out.println(list1.getLast());
		System.out.println(list1.peekLast());
		
	}
}
